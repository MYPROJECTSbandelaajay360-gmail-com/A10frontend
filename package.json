{
  "name": "musterbook-portal",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "node --max-old-space-size=4096 ./node_modules/next/dist/bin/next dev -p 3000",
    "build": "prisma generate && next build",
    "start": "next start -p 3000",
    "prod": "NODE_ENV=production next start -p 3000",
    "lint": "next lint",
    "postinstall": "prisma generate",
    "pm2:start": "pm2 start ecosystem.config.js",
    "pm2:stop": "pm2 stop musterbook-frontend",
    "pm2:restart": "pm2 restart musterbook-frontend",
    "pm2:reload": "pm2 reload musterbook-frontend",
    "pm2:logs": "pm2 logs musterbook-frontend",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "npx prisma migrate deploy",
    "db:seed": "npx tsx prisma/seed.ts",
    "db:reset": "prisma migrate reset",
    "db:studio": "prisma studio"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "prisma": {
    "seed": "npx tsx prisma/seed.ts"
  },
  "dependencies": {
    "@auth/prisma-adapter": "^2.11.1",
    "@prisma/client": "^5.22.0",
    "@react-spring/web": "^10.0.3",
    "bcrypt": "^6.0.0",
    "bcryptjs": "^3.0.3",
    "date-fns": "^4.1.0",
    "framer-motion": "^12.34.0",
    "jsonwebtoken": "^9.0.3",
    "jspdf": "^4.0.0",
    "jspdf-autotable": "^5.0.7",
    "lucide-react": "^0.562.0",
    "mongoose": "^9.1.6",
    "next": "14.2.12",
    "next-auth": "^4.24.13",
    "prisma": "^5.22.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "recharts": "^3.7.0"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^22.19.7",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "autoprefixer": "^10.0.1",
    "eslint": "9.39.2",
    "eslint-config-next": "16.1.1",
    "postcss": "^8",
    "tailwindcss": "^3.4.1",
    "ts-node": "^10.9.2",
    "typescript": "^5"
  }
}
